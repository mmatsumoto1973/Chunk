        <servlet-name>OAuth2Client_AuthCode_GoogleCLWebCallbackServlet</servlet-name>
        <url-pattern>/authcode_googlecl_webcb</url-pattern>
    </servlet-mapping>
    <servlet>
        <display-name>OAuth2Client_ClientCredentials_GoogleCLSAServlet</display-name>
        <servlet-name>OAuth2Client_ClientCredentials_GoogleCLSAServlet</servlet-name>
        <servlet-class>jp.co.saison.hulft.oauth20.google.OAuth2Client_ClientCredentials_GoogleCLSAServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OAuth2Client_ClientCredentials_GoogleCLSAServlet</servlet-name>
        <url-pattern>/clientcredencials_googlecl_sa</url-pattern>
